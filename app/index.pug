		.controlPanel
			.controlPanel__button
		.controlPanelPopup
			.controlPanelPopup__opacityLayer
			.controlPanelPopup__header
				i.fas.fa-angle-down
			.controlPanelPopup__content
				mixin controlPanelCircleIconsBoxMix(iconsBoxModificationClass, firstNameCircleIconMix, secondNameCircleIconMix, thirdNameCircleIconMix, fourthNameCircleIconMix)
					.controlPanelPopup__contentItem
						.controlPanelPopup__iconsBox(class=`${iconsBoxModificationClass}`)
							.controlPanelPopup__circleIconWrap
								.controlPanelPopup__circleIcon
									i(class=`${firstNameCircleIconMix}`)
							.controlPanelPopup__circleIconWrap
								.controlPanelPopup__circleIcon
									i(class=`${secondNameCircleIconMix}`)
							.controlPanelPopup__circleIconWrap
								.controlPanelPopup__circleIcon
									i(class=`${thirdNameCircleIconMix}`)
							.controlPanelPopup__circleIconWrap
								.controlPanelPopup__circleIcon
									i(class=`${fourthNameCircleIconMix}`)
				mixin controlPanelSingleIconBoxMix(iconsBoxModificationClass)
					.controlPanelPopup__contentItem
						.controlPanelPopup__iconsBox(class=`${iconsBoxModificationClass}`)
				mixin controlPanelTripleIconBoxMix(firstNameSquareIconMix, secondNameSquareIconMix, firstNameHorizontalIconMix)
					.controlPanelPopup__contentItem
						.controlPanelPopup__iconsBox
							.controlPanelPopup__squareIcon
								i(class=`${firstNameSquareIconMix}`)
							.controlPanelPopup__squareIcon
								i(class=`${secondNameSquareIconMix}`)
							.controlPanelPopup__horizontalIcon
								i(class=`${firstNameHorizontalIconMix}`)
				mixin controlPanelVerticalIconBoxMix(firstNameVerticalIconMix, secondNameVerticalIconMix)
					.controlPanelPopup__contentItem
						.controlPanelPopup__iconsBox
							.controlPanelPopup__verticalIcon
								i(class=`${firstNameVerticalIconMix}`)
							.controlPanelPopup__verticalIcon
								i(class=`${secondNameVerticalIconMix}`)
				mixin controlPanelQuadrupleIconBoxMix(firstNameSquareIconMix, secondNameSquareIconMix, thirdNameSquareIconMix, fourthNameSquareIconMix)
					.controlPanelPopup__contentItem
						.controlPanelPopup__iconsBox
							.controlPanelPopup__squareIcon
								i(class=`${firstNameSquareIconMix}`)
							.controlPanelPopup__squareIcon
								i(class=`${secondNameSquareIconMix}`)
							.controlPanelPopup__squareIcon
								i(class=`${thirdNameSquareIconMix}`)
							.controlPanelPopup__squareIcon
								i(class=`${fourthNameSquareIconMix}`)
				mixin controlPanelHorizontalIconBoxMix(firstNameHorizontalIconMix, secondNameHorizontalIconMix)
					.controlPanelPopup__contentItem
						.controlPanelPopup__iconsBox
							.controlPanelPopup__horizontalIcon
								i(class=`${firstNameHorizontalIconMix}`)
							.controlPanelPopup__horizontalIcon
								i(class=`${secondNameHorizontalIconMix}`)
				+controlPanelCircleIconsBoxMix('controlPanelPopup__iconsBox_borderRad controlPanelPopup__iconsBox_bg', 'fas fa-plane', 'fas fa-broadcast-tower', 'fas fa-wifi', 'fab fa-bluetooth-b')
				+controlPanelSingleIconBoxMix('controlPanelPopup__iconsBox_borderRad controlPanelPopup__iconsBox_bg')
				+controlPanelTripleIconBoxMix('fas fa-unlock', 'fas fa-moon', 'fas fa-desktop')
				+controlPanelVerticalIconBoxMix('fas fa-sun', 'fas fa-volume-up')
				+controlPanelQuadrupleIconBoxMix('fas fa-lightbulb', 'fas fa-map-marker-alt', 'fas fa-calculator', 'fas fa-camera')
				+controlPanelQuadrupleIconBoxMix('far fa-image', 'far fa-clock', 'fab fa-apple', 'fas fa-car')
				+controlPanelHorizontalIconBoxMix('fab fa-apple-pay', 'fab fa-cc-visa')
